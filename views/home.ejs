

    <section id="feed-posts">
        <h4>Posts</h4>
      <%if(locals.user){%>
        <form action="/posts/create" id="new-post-form" method="post">
            <textarea name="content" id="" cols="30" rows="3" placeholder="Type Comment" required>
            </textarea>
           <input type="submit" value="Post " id="submit">
                   </form>
      <%}%>

<div id="post-list">

    
    <ul>
        <% for(post of Post) {%>
     <%- include('_post') -%>
        <%}%>
    </ul>
</div>
    </section>